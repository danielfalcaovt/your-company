{
  "compilerOptions": {
      "target": "ES6",
      "lib": [
          "DOM",
          "DOM.Iterable",
          "ESNext"
      ],
      "esModuleInterop": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "jsx": "react",
      "rootDir": "src",
      "baseUrl": "src",
      "allowJs": true,
      "strict": true,
      "strictNullChecks": false,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "paths": {
          "@/*": ["*"]
      }
  },
  "include": [
      "src"
  ]
}